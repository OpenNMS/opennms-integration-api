{
  "name": "ui-extension",
  "version": "0.0.0",
  "files": [
    "dist"
  ],
  "main": "./dist/uiextension.umd.js",
  "module": "./dist/uiextension.es.js",
  "exports": {
    ".": {
      "import": "./dist/uiextension.es.js",
      "require": "./dist/uiextension.umd.js"
    }
  },
  "scripts": {
    "dev": "vite --config vite.config.dev.ts",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite build --mode development && vite preview --port 5002"
  },
  "dependencies": {
    "pinia": "^2.0.14",
    "vite-plugin-externals": "^0.5.0",
    "vue": "^3.2.37",
    "vue-router": "^4.0.16"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@vitejs/plugin-vue": "^2.2.0",
    "typescript": "^4.5.4",
    "vite": "^2.8.0",
    "vue-tsc": "^0.38.1"
  }
}
